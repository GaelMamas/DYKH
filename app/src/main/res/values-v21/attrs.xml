        <attr name="arcbutton_angle" format="float" />
        <attr name="arcbutton_previous" format="float" />
        <attr name="arcbutton_little_radius" format="float" />
        <attr name="arcbutton_great_radius" format="float" />

        <attr name="arcbutton_text_color" format="color" />
